<script>
  window.foxState = { step: 1, rating: 0 };

  // ====== THAY LINK GOOGLE SHEETS CỦA BẠN Ở ĐÂY ======
  var GOOGLE_SCRIPT_URL = 'https://script.google.com/macros/s/AKfycbwUk5TWTQEQLYMn5lj_S1p5gcLBNHxr0urcZCbVuVb3InEvhn79o5v00W8Cn5V44RZy/exec';

  window.foxUpdateUI = function() {
    var step = window.foxState.step;
    
    var steps = document.querySelectorAll('#fox-review-form .fox-step');
    steps.forEach(function(el) {
      el.classList.remove('active');
      if (parseInt(el.getAttribute('data-step')) === step) el.classList.add('active');
    });

    var dots = document.querySelectorAll('#fox-review-form .fox-dot');
    dots.forEach(function(el, idx) {
      el.classList.remove('active', 'completed');
      if (idx + 1 === step) el.classList.add('active');
      if (idx + 1 < step) el.classList.add('completed');
    });

    var btnBack = document.getElementById('fox-back');
    var btnSkip = document.getElementById('fox-skip');
    var btnNext = document.getElementById('fox-next');
    var btnSubmit = document.getElementById('fox-submit');
    var navFooter = document.getElementById('fox-nav');

    if (step === 5) {
      navFooter.style.display = 'none';
      return;
    } else {
      navFooter.style.display = 'flex';
    }

    btnBack.style.visibility = step > 1 ? 'visible' : 'hidden';
    btnSkip.style.display = 'none';
    btnNext.style.display = 'none';
    btnSubmit.style.display = 'none';

    if (step === 1) {
      // Click sao để chuyển
    } else if (step === 2) {
      btnSkip.style.display = 'block';
    } else if (step === 3) {
      btnNext.style.display = 'block';
      var text = document.getElementById('fox-text').value.trim();
      btnNext.disabled = !text;
    } else if (step === 4) {
      btnSubmit.style.display = 'block';
    }
  };

  window.foxNext = function() {
    if (window.foxState.step < 4) {
      window.foxState.step++;
      window.foxUpdateUI();
    }
  };

  window.foxPrev = function() {
    if (window.foxState.step > 1) {
      window.foxState.step--;
      window.foxUpdateUI();
    }
  };

  window.foxRate = function(starVal) {
    window.foxState.rating = starVal;

    var stars = document.querySelectorAll('#fox-review-form .fox-star');
    stars.forEach(function(s, idx) {
      if (idx < starVal) {
        s.classList.add('active');
        s.textContent = '★';
      } else {
        s.classList.remove('active');
        s.textContent = '☆';
      }
    });

    setTimeout(function() {
      window.foxNext();
    }, 300);
  };

  // Listener cho textarea
  document.getElementById('fox-text').addEventListener('input', function() {
    if (window.foxState.step === 3) {
      var btnNext = document.getElementById('fox-next');
      btnNext.disabled = !this.value.trim();
    }
  });

  window.foxSubmit = function() {
    var name = document.getElementById('fox-name').value.trim();
    var lastname = document.getElementById('fox-lastname').value.trim();
    var email = document.getElementById('fox-email').value.trim();
    var review = document.getElementById('fox-text').value.trim();
    var submitBtn = document.getElementById('fox-submit');
    
    // Validate
    if (!name || !email) { 
      alert('Please fill in required fields'); 
      return; 
    }

    // Hiệu ứng đang gửi
    var originalText = submitBtn.innerText;
    submitBtn.innerText = 'Sending...';
    submitBtn.disabled = true;

    // Gửi về Google Sheets
    // Header trong Sheet phải là: Date | Name | Email | Rating | Review
    fetch(GOOGLE_SCRIPT_URL, {
      method: 'POST',
      mode: 'no-cors',
      headers: {
        'Content-Type': 'application/x-www-form-urlencoded'
      },
      body: 'Name=' + encodeURIComponent(name + ' ' + lastname) + 
            '&Email=' + encodeURIComponent(email) + 
            '&Rating=' + encodeURIComponent(window.foxState.rating) + 
            '&Review=' + encodeURIComponent(review)
    })
    .then(function() {
      console.log('Submitted:', { 
        rating: window.foxState.rating, 
        review: review, 
        name: name + ' ' + lastname, 
        email: email 
      });
      
      // Chuyển sang màn Thank You
      window.foxState.step = 5;
      window.foxUpdateUI();
    })
    .catch(function(error) {
      console.error('Error:', error);
      alert('Something went wrong. Please try again.');
    })
    .finally(function() {
      submitBtn.innerText = originalText;
      submitBtn.disabled = false;
    });
  };

  window.foxClose = function() {
    // Reset Form
    window.foxState.step = 1;
    window.foxState.rating = 0;
    document.getElementById('fox-text').value = '';
    document.getElementById('fox-name').value = '';
    document.getElementById('fox-lastname').value = '';
    document.getElementById('fox-email').value = '';
    var stars = document.querySelectorAll('#fox-review-form .fox-star');
    stars.forEach(function(s) { s.classList.remove('active'); s.textContent = '☆'; });
    window.foxUpdateUI();

    // Đóng popup Foxify
    var formElement = document.getElementById('fox-review-form');
    var foxModal = formElement.closest('.x-modal'); 

