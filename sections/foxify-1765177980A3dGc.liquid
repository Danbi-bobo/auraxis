<div class="foxify-review-wrapper">
  
  {%- assign review_list = product.metafields.custom.featured_reviews.value -%}
  {%- assign review_count = review_list | size -%}

  <div class="review-header-custom">
    <div class="review-summary">
      <span class="stars">★★★★★</span>
      <span>
        {% if review_count > 0 %}
          {{ review_count }} Reviews
        {% else %}
          0 Reviews
        {% endif %}
      </span>
    </div>  
  </div>

  <div class="masonry-grid-custom">
    {%- if review_list != blank -%}
      {%- for review in review_list -%}
        <div class="review-card">
          
