            <div class="review-image-container">
              <img src="{{ review.review_image | image_url: width: 600 }}" loading="lazy" width="{{ review.review_image.width }}" height="{{ review.review_image.height }}" alt="{{ review.author_name }}">
            </div>
          {%- endif -%}

          <div class="reviewer-info">
            <div>
              <div class="reviewer-name">
                {{ review.author_name }}
                {%- if review.verified == true -%}
                  <svg class="verified-badge" viewBox="0 0 24 24" fill="currentColor"><path d="M12 2C6.48 2 2 6.48 2 12s4.48 10 10 10 10-4.48 10-10S17.52 2 12 2zm-2 15l-5-5 1.41-1.41L10 14.17l7.59-7.59L19 8l-9 9z"></path></svg>
                {%- endif -%}
              </div>
              <div class="review-date">
                {% if review.review_date %}
                  {{ review.review_date | date: "%m/%d/%Y" }}
                {% endif %}
              </div>
            </div>
          </div>

          <div class="star-rating">
            {%- assign rating_obj = review.rating.value -%}
            
            {%- if rating_obj.rating -%}
              {%- assign rating_num = rating_obj.rating | plus: 0 -%}
            {%- else -%}
              {%- assign rating_num = rating_obj | plus: 0 -%}
            {%- endif -%}

            {%- if rating_num == 0 -%}{%- assign rating_num = 5 -%}{%- endif -%}
            
            {%- for i in (1..5) -%}
              {%- if i <= rating_num -%}
                ★
              {%- else -%}
                <span style="opacity: 0.3">★</span>
              {%- endif -%}
            {%- endfor -%}
          </div>

          <div class="review-body">
            {{ review.review_body | newline_to_br }}
          </div>

          {%- if review.product_variant_text != blank -%}
            <div class="item-type">
              <strong>Item type:</strong>
              {{ review.product_variant_text }}
            </div>
          {%- endif -%}
          
        </div>
      {%- endfor -%}
    
    {%- else -%}
        <div class="empty-state">
          <p>No featured reviews connected.</p>
        </div>
    {%- endif -%}
  </div>
