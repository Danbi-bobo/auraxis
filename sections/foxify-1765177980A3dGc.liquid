        if (window.getComputedStyle(foxModal).display !== 'none') {
          foxModal.style.display = 'none';
          foxModal.classList.remove('active', 'open', 'is-visible'); 
        }
      }, 100);
    }
  };
  
  window.foxUpdateUI();
